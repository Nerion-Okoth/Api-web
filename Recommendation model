            "Recommended opportunities for John Doe:\n",
            "['Community Teaching', 'Fundraising Event', 'Social Media Campaign']\n",
            "--------------------------------------------------\n",
            "Recommended opportunities for Mary W.:\n",
            "['NGO Website Development']\n",
            "--------------------------------------------------\n",
            "Recommended opportunities for Peter K.:\n",
            "['Fundraising Event', 'Community Teaching', 'Social Media Campaign']\n",
            "--------------------------------------------------\n",
            "Recommended opportunities for Alice M.:\n",
            "['First Aid Camp']\n",
            "--------------------------------------------------\n",
            "Recommended opportunities for David O.:\n",
            "['Social Media Campaign', 'Community Teaching', 'Fundraising Event']\n",
            "--------------------------------------------------\n"
          ]
        }
      ],
      "source": [
        "# Step 1: Install dependencies (uncomment if needed)\n",
        "# !pip install pandas scikit-learn\n",
        "\n",
        "# Step 2: Import libraries\n",
        "import pandas as pd\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "\n",
        "# Step 3: Create sample volunteers data\n",
        "volunteers = pd.DataFrame({\n",
        "    'volunteer_id': [1001, 1002, 1003, 1004, 1005],\n",
        "    'name': ['John Doe', 'Mary W.', 'Peter K.', 'Alice M.', 'David O.'],\n",
        "    'skills': [\n",
        "        'teaching, communication, leadership',\n",
        "        'coding, web development, teamwork',\n",
        "        'fundraising, event planning, leadership',\n",
        "        'healthcare, first aid, volunteering',\n",
        "        'photography, social media, design'\n",
        "    ],\n",
        "    'location': ['Nairobi', 'Mombasa', 'Nairobi', 'Kisumu', 'Nairobi']\n",
        "})\n",
        "\n",
        "# Step 4: Create sample opportunities data\n",
        "opportunities = pd.DataFrame({\n",
        "    'opportunity_id': [2001, 2002, 2003, 2004, 2005],\n",
        "    'title': [\n",
        "        'Community Teaching',\n",
        "        'NGO Website Development',\n",
        "        'Fundraising Event',\n",
        "        'First Aid Camp',\n",
        "        'Social Media Campaign'\n",
        "    ],\n",
        "    'required_skills': [\n",
        "        'teaching, communication',\n",
        "        'web development, design',\n",
        "        'fundraising, event planning',\n",
        "        'healthcare, first aid',\n",
        "        'social media, photography'\n",
        "    ],\n",
        "    'location': ['Nairobi', 'Mombasa', 'Nairobi', 'Kisumu', 'Nairobi']\n",
        "})\n",
        "\n",
        "# Step 5: TF-IDF vectorization of skills\n",
        "vectorizer = TfidfVectorizer(stop_words='english')\n",
        "tfidf_volunteers = vectorizer.fit_transform(volunteers['skills'])\n",
        "tfidf_opportunities = vectorizer.transform(opportunities['required_skills'])\n",
        "\n",
        "# Step 6: Compute cosine similarity between volunteers and opportunities\n",
        "similarity = cosine_similarity(tfidf_volunteers, tfidf_opportunities)\n",
        "\n",
        "# Step 7: Function to recommend top opportunities for a volunteer\n",
        "def recommend_opportunities(volunteer_id, top_n=3):\n",
        "    # Find the volunteer index\n",
        "    v_index = volunteers[volunteers['volunteer_id'] == volunteer_id].index[0]\n",
        "\n",
        "    # Compute similarity scores\n",
        "    sim_scores = list(enumerate(similarity[v_index]))\n",
        "\n",
        "    # Filter by location\n",
        "    volunteer_location = volunteers.loc[v_index, 'location']\n",
        "    loc_filtered = [(i, score) for i, score in sim_scores if opportunities.loc[i, 'location'] == volunteer_location]\n",
        "\n",
        "    # Sort by similarity score\n",
        "    loc_filtered = sorted(loc_filtered, key=lambda x: x[1], reverse=True)\n",
        "\n",
        "    # Get top N recommendations\n",
        "    top_recommendations = [opportunities.iloc[i[0]]['title'] for i in loc_filtered[:top_n]]\n",
        "\n",
        "    return top_recommendations\n",
        "\n",
        "# Step 8: Test the function\n",
        "for vid in volunteers['volunteer_id']:\n",
        "    print(f\"Recommended opportunities for {volunteers[volunteers['volunteer_id']==vid]['name'].values[0]}:\")\n",
        "    print(recommend_opportunities(vid))\n",
        "    print(\"-\" * 50)\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Step 7 (updated): Function to recommend top opportunities with similarity scores\n",
        "def recommend_opportunities_with_scores(volunteer_id, top_n=3):\n",
        "    # Find volunteer index\n",
        "    v_index = volunteers[volunteers['volunteer_id'] == volunteer_id].index[0]\n",
        "\n",
        "    # Compute similarity scores\n",
        "    sim_scores = list(enumerate(similarity[v_index]))\n",
        "\n",
        "    # Filter by location\n",
        "    volunteer_location = volunteers.loc[v_index, 'location']\n",
        "    loc_filtered = [(i, score) for i, score in sim_scores if opportunities.loc[i, 'location'] == volunteer_location]\n",
        "\n",
        "    # Sort by similarity score\n",
        "    loc_filtered = sorted(loc_filtered, key=lambda x: x[1], reverse=True)\n",
        "\n",
        "    # Take top N\n",
        "    top_filtered = loc_filtered[:top_n]\n",
        "\n",
        "    # Build a DataFrame for display\n",
        "    recs = pd.DataFrame({\n",
        "        'Opportunity': [opportunities.iloc[i[0]]['title'] for i in top_filtered],\n",
        "        'Location': [opportunities.iloc[i[0]]['location'] for i in top_filtered],\n",
        "        'Skill Match %': [round(i[1]*100, 2) for i in top_filtered]  # convert to percentage\n",
        "    })\n",
        "\n",
        "    return recs\n",
        "\n",
        "# Step 8: Display recommendations for all volunteers\n",
        "for vid in volunteers['volunteer_id']:\n",
        "    print(f\"Recommended opportunities for {volunteers[volunteers['volunteer_id']==vid]['name'].values[0]}:\\n\")\n",
        "    display(recommend_opportunities_with_scores(vid))\n",
        "    print(\"-\" * 70)\n"
      ],
