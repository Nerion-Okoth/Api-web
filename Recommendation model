    {
      "cell_type": "code",
      "source": [
        "# Step 1: Define nearby cities mapping\n",
        "nearby_cities = {\n",
        "    'Nairobi': ['Nairobi', 'Kiambu', 'Machakos'],   # Nairobi and nearby counties\n",
        "    'Mombasa': ['Mombasa', 'Kwale', 'Kilifi'],\n",
        "    'Kisumu': ['Kisumu', 'Homa Bay', 'Siaya']\n",
        "}\n",
        "\n",
        "# Step 2: Updated recommendation function with nearby cities\n",
        "def recommend_opportunities_nearby(volunteer_id, top_n=3):\n",
        "    # Find volunteer index\n",
        "    v_index = volunteers[volunteers['volunteer_id'] == volunteer_id].index[0]\n",
        "\n",
        "    # Compute similarity scores\n",
        "    sim_scores = list(enumerate(similarity[v_index]))\n",
        "\n",
        "    # Get the volunteer's city and nearby cities\n",
        "    volunteer_city = volunteers.loc[v_index, 'location']\n",
        "    allowed_cities = nearby_cities.get(volunteer_city, [volunteer_city])\n",
        "\n",
        "    # Filter opportunities by allowed cities\n",
        "    loc_filtered = [(i, score) for i, score in sim_scores if opportunities.loc[i, 'location'] in allowed_cities]\n",
        "\n",
        "    # Sort by similarity score\n",
        "    loc_filtered = sorted(loc_filtered, key=lambda x: x[1], reverse=True)\n",
        "\n",
        "    # Take top N\n",
        "    top_filtered = loc_filtered[:top_n]\n",
        "\n",
        "    # Build a DataFrame for display\n",
        "    recs = pd.DataFrame({\n",
        "        'Opportunity': [opportunities.iloc[i[0]]['title'] for i in top_filtered],\n",
        "        'Location': [opportunities.iloc[i[0]]['location'] for i in top_filtered],\n",
        "        'Skill Match %': [round(i[1]*100, 2) for i in top_filtered]  # convert to percentage\n",
        "    })\n",
        "\n",
        "    return recs\n",
        "\n",
        "# Step 3: Display recommendations for all volunteers\n",
        "for vid in volunteers['volunteer_id']:\n",
        "    print(f\"Recommended opportunities for {volunteers[volunteers['volunteer_id']==vid]['name'].values[0]}:\\n\")\n",
        "    display(recommend_opportunities_nearby(vid))\n",
        "    print(\"-\" * 70)\n"
      ],
